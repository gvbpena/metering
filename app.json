{
    "expo": {
        "name": "metering",
        "slug": "metering",
        "version": "1.0.3",
        "orientation": "portrait",
        "icon": "./assets/images/genius-image(icon).png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.gvbpena.metering",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/genius-image.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.gvbpena.metering"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/genius-image.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/genius-image.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-sqlite"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "650801fb-243c-4aad-8a55-2739469d0ba0"
            }
        },
        "runtimeVersion": {
            "policy": "appVersion"
        },
        "updates": {
            "url": "https://u.expo.dev/650801fb-243c-4aad-8a55-2739469d0ba0"
        }
    }
}
